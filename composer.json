{
  "name": "miaoxing/plugin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Plugin Core",
  "keywords": [
    "miaoxing",
    "plugin"
  ],
  "license": "MIT",
  "require": {
    "wei/wei": "^0.9.30",
    "doctrine/inflector": "^1.4|^2.0",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "miaoxing/dev": "^6.1.1",
    "nette/php-generator": "^3.4"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Miaoxing\\Plugin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Plugin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
