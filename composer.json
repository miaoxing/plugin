{
  "name": "miaoxing/plugin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Plugin Core",
  "keywords": [
    "miaoxing",
    "plugin"
  ],
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "ext-openssl": "*",
    "wei/wei": "^0.17.5",
    "doctrine/inflector": "^1.4|^2.0",
    "adhocore/jwt": "^1.1.2",
    "fakerphp/faker": "^1.14",
    "composer/installers": "^1.11.0",
    "dragonmantank/cron-expression": "^2||^3"
  },
  "require-dev": {
    "miaoxing/dev": "^9.1.1",
    "nette/php-generator": "^3.4",
    "symfony/var-dumper": ">=5"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Miaoxing\\Plugin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Plugin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 3.x-dev",
      "wei/wei": "twinh/wei as 0.17.x-dev"
    }
  }
}
