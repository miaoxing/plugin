{
  "name": "miaoxing/plugin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Plugin Core",
  "keywords": [
    "miaoxing",
    "plugin"
  ],
  "license": "MIT",
  "require": {
    "wei/wei": "dev-master@dev",
    "doctrine/inflector": "^v1.1.0",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^5.0.0@beta",
    "phpstan/phpstan": "^0.12.29",
    "nette/php-generator": "^3.4",
    "phpstan/phpstan-phpunit": "^0.12.11"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Miaoxing\\Plugin\\": "src",
      "Wei\\": "vendor/wei/wei/lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Plugin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
