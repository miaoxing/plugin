{
  "name": "miaoxing/plugin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Plugin Core",
  "keywords": [
    "miaoxing",
    "plugin"
  ],
  "license": "MIT",
  "require": {
    "wei/wei": "dev-master@dev",
    "doctrine/inflector": "^v1.1.0",
    "miaoxing/services": "dev-master@dev",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "miaoxing/build": "dev-master@dev",
    "miaoxing/coding-standards": "0.9.6",
    "phpunit/phpunit": "^8|^9"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Plugin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Plugin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
